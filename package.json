{
  "name": "@radya/nuxt-proxy-party",
  "version": "1.1.2",
  "description": "Nuxt HTTP proxy based on H3",
  "repository": "radyakaze/nuxt-proxy-party",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./core": {
      "import": "./dist/core.mjs",
      "require": "./dist/core.cjs",
      "types": "./dist/core.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "template",
    "bin"
  ],
  "bin": "./bin/index.mjs",
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run prepack && changelogen --release --no-output && npm publish --access public && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.4",
    "citty": "^0.1.6"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.12.4",
    "@nuxt/test-utils": "^3.14.0",
    "@types/node": "^20.14.11",
    "@vitest/ui": "^2.0.5",
    "changelogen": "^0.5.5",
    "eslint": "^9.7.0",
    "nuxt": "^3.12.4",
    "typescript": "latest",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.26"
  },
  "packageManager": "yarn@4.4.0"
}
